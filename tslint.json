{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
	  "arrow-parens": false,
	  "forin": false,
	  "member-access": [ true, "no-public" ],
	  "member-ordering": false,
	  "no-empty": false,
	  "no-string-literal": false,
	  "object-literal-sort-keys": false,
	  "quotemark": [ true, "single" ],
	  "trailing-comma": [true, { "esSpecCompliant": true } ]
  },
  "rulesDirectory": []
}
